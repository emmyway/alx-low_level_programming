#include <stdio.h>

/**
 * add - add two integers
 * @a: first integer
 * @b: the second integer
 * Return: result
 */
int add(int a, int b)
{
	return (a + b);
}

/**
 * sub - subtrac two integers
 * @a: first integer
 * @b: the second integer
 * Return: result
 */
int sub(int a, int b)
{
	return (a - b);
}

/**
 * mul - multiply two integers
 * @a: first integer
 * @b: the second integer
 * Return: result
 */
int mul(int a, int b)
{
	return (a * b);
}

/**
 * div - divides two integers
 * @a: first integer
 * @b: the second integer
 * Return: result
 */
int div(int a, int b)
{
	return (a / b);
}

/**
 * mod - find modulos of two integers
 * @a: first integer
 * @b: the second integer
 * Return: result
 */
int mod(int a, int b)
{
	return (a % b);
}
